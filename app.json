{
  "name": "LunaMusicBot",
  "description": "Telegram'ınızın Grup Sesli Sohbeti'nde şarkı çalmak için Açık Kaynaklı bot. PyTgCalls tarafından desteklenmektedir.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/doktorceset/LunaMusicPlayBotV1",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram oturum dizesi",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bot Token @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "MusicPlayer Bot Adınız.",
      "required": false,
      "value": "LunaNews Müzik Bot"
    },
    "API_ID": {
      "description": "App ID my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Yönetici ID listesi.",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "İndirmeler için maksimum ses süresi sınırı (dakika).",
      "required": true,
      "value": "10"
    }
  }
}
